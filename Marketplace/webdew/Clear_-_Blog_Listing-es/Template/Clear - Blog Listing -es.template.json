                  "0" : {
                    "x" : 0,
                    "w" : 12,
                    "name" : "module_151456960811572",
                    "label" : "Blog Content",
                    "type" : "custom_widget",
                    "params" : {
                      "blog_listing_source" : "<div class=\"blog-section\">\n    <div class=\"blog-listing-wrapper cell-wrapper\">\n        <div class=\"blog-section clear--blog-design\">\n            <div class=\"blog-listing-wrapper cell-wrapper\">\n                {# simple_list_page indicates the \"blog/all\" page, which is a list of links to every blog post #}\n                <div class=\"post-listing{% if simple_list_page %}-simple{% endif %}\">\n                    {% if blog_author %}\n                    <div class=\"hs-author-profile\">\n                        <h4 class=\"hs-author-name\">{{ blog_author.display_name }}</h4>\n                        {% if blog_author.avatar %} <div class=\"hs-author-avatar\"> <img src=\"{{ blog_author.avatar }}\" alt=\"{{ blog_author.display_name }}\"> </div> {% endif %}\n                        <div class=\"hs-author-bio\">{{ blog_author.bio }}</div>\n                        {% if blog_author.has_social_profiles %}\n                        <div class=\"hs-author-social-section\">\n                            <span class=\"hs-author-social-label\">Find me on:</span>\n                            <div class=\"hs-author-social-links\">\n                                {% if blog_author.facebook %}\n                                <a href=\"{{ blog_author.facebook }}\" target=\"_blank\" class=\"hs-author-social-link hs-social-facebook\">&nbsp;</a>\n                                {% endif %}\n                                {% if blog_author.linkedin %}\n                                <a href=\"{{ blog_author.linkedin }}\" target=\"_blank\" class=\"hs-author-social-link hs-social-linkedin\">&nbsp;</a>\n                                {% endif %}\n                                {% if blog_author.twitter %}\n                                <a href=\"{{ blog_author.twitter }}\" target=\"_blank\" class=\"hs-author-social-link hs-social-twitter\">&nbsp;</a>\n                                {% endif %}\n                                {% if blog_author.google_plus %}\n                                <a href=\"{{ blog_author.google_plus }}?rel=author\" target=\"_blank\" class=\"hs-author-social-link hs-social-google-plus\">&nbsp;</a>\n                                {% endif %}\n                            </div>\n                        </div>\n                        {% endif %}\n                    </div>\n                    <h3 class=\"hs-author-listing-header\">Recent Posts</h3>\n                    {% endif %}\n                    {% for content in contents %}\n                    <div class=\"post-item\">\n                        {% if not simple_list_page %}\n                        <div class=\"post-body clearfix {% if content.post_list_summary_featured_image == false %}post_item_full{% endif %}\">\n                            <div class=\"post-blog-content\">\n                                <div class=\"blog--inner-space\">\n                                    <div class=\"post-header\">\n                                        <h4><a href=\"{{content.absolute_url}}\">{{ content.name }}</a></h4>\n                                    </div>\n                                    <div class=\"blog--tag-author-coloumn\">\n                                         <div class=\"topic\">\n                                            {% if content.topic_list %}\n                                            <p id=\"hubspot-topic_data\">\n                                                {% for topic in content.topic_list %}\n                                                <i class=\"fa fa-tags\" aria-hidden=\"true\"></i>\n                                                <a class=\"topic-link\" href=\"{{ blog_tag_url(group.id, topic.slug) }}\">{{ topic.name }}</a>{% if not loop.last %},{% endif %}\n                                                {% endfor %}\n                                            </p>\n                                            {% endif %}\n                                        </div>\n                                        <div id=\"hubspot-author_data\" class=\"hubspot-editable\" data-hubspot-form-id=\"author_data\" data-hubspot-name=\"Blog Author\">\n                                            <div class=\"blog--author\">\n                                                {%if content.blog_post_author %}\n                                                <i class=\"fa fa-user\" aria-hidden=\"true\"></i><a class=\"author-link\" href=\"{{ blog_author_url(group.id, content.blog_post_author.slug) }}\">\n                                                    {{ content.blog_post_author.display_name }}</a>\n                                                {% endif %}\n                                            </div>\n                                        </div>\n                                       \n                                    </div>\n                                    <div class=\"blog_content_area\">\n                                        <p>{{ content.post_list_content|truncatehtml(120, '...', false)}}</p>\n                                        {% if content_group.show_summary_in_listing %}\n                                        <a class=\"more-link\" href=\"{{ content.absolute_url }}\">Read More</a>\n                                        {% endif %}\n                                    </div>\n                                </div>\n                            </div>\n                            {% if content.post_list_summary_featured_image %}\n                            <div class=\"blog_featured_img\">\n                                <div class=\"hs-featured-image-wrapper\">\n                                    <a href=\"{{content.absolute_url}}\" title=\"\" class=\"hs-featured-image-link\">\n                                        <img src=\"{{ content.post_list_summary_featured_image }}\" class=\"hs-featured-image\" alt=\"{{ content.featured_image_alt_text | escape }}\">\n                                    </a>\n                                </div>\n                            </div>\n                            {% endif %}\n                        </div>\n                        {% else %}\n                        <h4 class=\"post-listing-simple\"><a href=\"{{content.absolute_url}}\">{{ content.name }}</a></h4>\n                        {% endif %}\n                    </div>\n                    {% endfor %}\n                </div>\n                <div class=\"pagination wrapper flex-row space-between\">\n                    {% if not simple_list_page %}\n                    <div class=\"pages-navigation flex-row align-center\">\n                        {% if current_page_num > 1 %}\n                        {% set previous = current_page_num - 1 %}\n                        <a rel=\"nofollow\" href=\"{{ blog_page_link(previous) }}\">\n                            <span class=\"btn\"><i class=\"fa fa-angle-double-left\" aria-hidden=\"true\"></i></span>\n                        </a>\n                        {% endif %} {% if current_page_num %}\n                        <a href=\"{{ content.post_slug }}\" class=\"page-number btn current\">\n                            <span>{{ current_page_num }}</span>\n                        </a>\n                        {% endif %} {% if next_page_num %}\n                        <a href=\"{{ blog_page_link(next_page_num) }}\" class=\"page-number btn\">\n                            <span>{{next_page_num }}</span>\n                        </a>\n                        <a rel=\"nofollow\" href=\"{{ blog_page_link(next_page_num) }}\">\n                            <span class=\"btn\"><i class=\"fa fa-angle-double-right\" aria-hidden=\"true\"></i></span>\n                        </a>\n                        {% endif %}\n                    </div>\n                    {% endif %}\n                </div>\n            </div>\n        </div>\n    </div>\n</div>",
                      "blog_post_source" : "<div class=\"blog-section blog-detail--clear\">\n    <div class=\"blog-post-wrapper cell-wrapper\">\n        <div class=\"blog-section\">\n            <div class=\"blog-post-wrapper cell-wrapper\">\n                <div class=\"section post-header\">\n                    <div class=\"blog_featured_img blog_detail\">\n                        {% if content.post_list_summary_featured_image %}\n                        <div class=\"hs-featured-image-wrapper\">\n                            <a href=\"{{content.absolute_url}}\" title=\"\" class=\"hs-featured-image-link\">\n                                <img src=\"{{ content.post_list_summary_featured_image }}\" class=\"hs-featured-image\" alt=\"{{ content.featured_image_alt_text | escape }}\">\n                            </a>\n                        </div>\n                        {% endif %}\n                    </div>\n                    <h1>{{ content.name }}</h1>\n                    <div id=\"hubspot-author_data\" class=\"hubspot-editable\" data-hubspot-form-id=\"author_data\" data-hubspot-name=\"Blog Author\">\n                        \n                        {% if content.blog_post_author %}\n                        {% if content.blog_post_author.avatar %} <div class=\"hs-author-avatar\"> <img src=\"{{ content.blog_post_author.avatar }}\" alt=\"{{ content.blog_post_author.display_name }}\"> </div> {% endif %}\n                        <div class=\"posted-by--blog\">\n                        <span class=\"hs-author-label\">Posted by </span>\n                        <a class=\"author-link\" href=\"{{ blog_author_url(group.id, content.blog_post_author.slug) }}\">{{ content.blog_post_author.display_name }}</a> on {{ content.publish_date_localized }}\n                        </div>\n                        {% endif %}\n                    </div>\n                </div>\n                <div class=\"section post-body\">\n                    {{ content.post_body }}\n                </div>\n                {% if content.topic_list %}\n                <p id=\"hubspot-topic_data\"> Topics:\n                    {% for topic in content.topic_list %}\n                    <a class=\"topic-link\" href=\"{{ blog_tag_url(group.id, topic.slug) }}\">{{ topic.name }}</a>{% if not loop.last %},{% endif %}\n                    {% endfor %}\n                </p>\n                {% endif %}\n                {% blog_social_sharing \"blog_social_sharing\" overrideable=False, label='Blog Social Sharing' %}\n            </div>\n        </div>\n    </div>\n</div>",
